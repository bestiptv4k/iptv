/* Color Variables */
:root {
    --warning-500: #f59e0b;
    --success-500: #10b981;
    --text-muted: #9ca3af;
}
</style>

<script>
// Interactive Device Compatibility JavaScript
document.addEventListener('DOMContentLoaded', function() {
    initInteractiveDeviceCompatibility();
    
    function initInteractiveDeviceCompatibility() {
        setupTabSwitching();
        setupDeviceDetection();
        setupExternalLinkTracking();
        setupPerformanceMonitoring();
    }
    
    // Tab Switching Functionality
    function setupTabSwitching() {
        const tabButtons = document.querySelectorAll('.tab-button');
        const tabContents = document.querySelectorAll('.tab-content');
        
        tabButtons.forEach(button => {
            button.addEventListener('click', function() {
                const targetTab = this.getAttribute('data-tab');
                
                // Remove active class from all buttons and contents
                tabButtons.forEach(btn => btn.classList.remove('active'));
                tabContents.forEach(content => content.classList.remove('active'));
                
                // Add active class to clicked button and corresponding content
                this.classList.add('active');
                document.getElementById(targetTab).classList.add('active');
                
                // Track tab selection
                if (typeof gtag !== 'undefined') {
                    gtag('event', 'device_tab_selection', {
                        'tab_name': targetTab,
                        'event_category': 'device_compatibility'
                    });
                }
                
                // Animate tab content
                const activeContent = document.getElementById(targetTab);
                activeContent.style.opacity = '0';
                activeContent.style.transform = 'translateY(20px)';
                
                setTimeout(() => {
                    activeContent.style.opacity = '1';
                    activeContent.style.transform = 'translateY(0)';
                }, 50);
                
                console.log(`üì± Device tab selected: ${targetTab}`);
            });
        });
    }
    
    // Enhanced Device Detection
    function setupDeviceDetection() {
        const userAgent = navigator.userAgent.toLowerCase();
        let detectedDevice = 'unknown';
        let recommendedTab = 'streaming'; // default
        
        if (userAgent.includes('iphone') || userAgent.includes('ipad')) {
            detectedDevice = 'ios';
            recommendedTab = 'mobile';
            highlightRecommendedDevice('iOS Devices');
        } else if (userAgent.includes('android')) {
            detectedDevice = 'android';
            recommendedTab = 'mobile';
            highlightRecommendedDevice('Android Devices');
        } else if (userAgent.includes('windows')) {
            detectedDevice = 'windows';
            recommendedTab = 'computer';
            highlightRecommendedDevice('Windows PC');
        } else if (userAgent.includes('mac')) {
            detectedDevice = 'macos';
            recommendedTab = 'computer';
            highlightRecommendedDevice('macOS');
        }
        
        // Show recommended tab badge
        if (recommendedTab !== 'streaming') {
            const recommendedTabButton = document.querySelector(`[data-tab="${recommendedTab}"]`);
            if (recommendedTabButton) {
                const badge = document.createElement('div');
                badge.innerHTML = 'üëÜ Recommended';
                badge.style.cssText = `
                    position: absolute;
                    top: -1px;
                    right: -1px;
                    background: #10b981;
                    color: white;
                    padding: 2px 6px;
                    border-radius: 8px;
                    font-size: 0.6rem;
                    font-weight: 700;
                    z-index: 10;
                    animation: pulse 2s infinite;
                `;
                recommendedTabButton.style.position = 'relative';
                recommendedTabButton.appendChild(badge);
            }
        }
        
        // Track detected device
        if (typeof gtag !== 'undefined') {
            gtag('event', 'device_detected', {
                'device_type': detectedDevice,
                'recommended_tab': recommendedTab,
                'event_category': 'device_compatibility'
            });
        }
    }
    
    // Highlight user's device
    function highlightRecommendedDevice(deviceName) {
        setTimeout(() => {
            const deviceCards = document.querySelectorAll('.device-card');
            deviceCards.forEach(card => {
                const cardTitle = card.querySelector('h4').textContent;
                if (cardTitle.includes(deviceName.split(' ')[0])) {
                    card.style.border = '2px solid #10b981';
                    card.style.boxShadow = '0 0 20px rgba(16, 185, 129, 0.3)';
                    
                    // Add "Your Device" badge
                    const badge = document.createElement('div');
                    badge.innerHTML = 'üì± Your Device';
                    badge.style.cssText = `
                        position: absolute;
                        top: 15px;
                        right: 15px;
                        background: #10b981;
                        color: white;
                        padding: 4px 8px;
                        border-radius: 6px;
                        font-size: 0.75rem;
                        font-weight: 600;
                        z-index: 10;
                        box-shadow: 0 2px 8px rgba(16, 185, 129, 0.3);
                    `;
                    card.style.position = 'relative';
                    card.appendChild(badge);
                }
            });
        }, 1000);
    }
    
    // Enhanced External Link Tracking
    function setupExternalLinkTracking() {
        document.querySelectorAll('.external-link').forEach(link => {
            link.addEventListener('click', function(e) {
                const linkText = this.textContent.trim();
                const deviceName = this.closest('.device-card').querySelector('h4').textContent;
                const tabName = this.closest('.tab-content').id;
                const linkType = this.classList.contains('primary') ? 'documentation' : 'purchase';
                
                // Analytics tracking
                if (typeof gtag !== 'undefined') {
                    gtag('event', 'external_link_click', {
                        'link_text': linkText,
                        'device_name': deviceName,
                        'tab_name': tabName,
                        'link_type': linkType,
                        'link_url': this.href,
                        'event_category': 'device_compatibility'
                    });
                }
                
                // Visual feedback
                this.style.transform = 'scale(0.95)';
                setTimeout(() => {
                    this.style.transform = '';
                }, 150);
                
                console.log(`üîó External link clicked: ${linkText} for ${deviceName} (${linkType})`);
            });
        });
    }
    
    // Performance Monitoring
    function setupPerformanceMonitoring() {
        window.addEventListener('load', () => {
            setTimeout(() => {
                const interactiveSection = document.querySelector('.device-compatibility-interactive');
                if (interactiveSection) {
                    console.log('üöÄ Interactive device compatibility loaded');
                    
                    if (typeof gtag !== 'undefined') {
                        gtag('event', 'interactive_compatibility_loaded', {
                            'event_category': 'performance',
                            'load_time': Date.now()
                        });
                    }
                }
            }, 1000);
        });
    }
    
    // Keyboard navigation support
    document.addEventListener('keydown', function(e) {
        const tabButtons = document.querySelectorAll('.tab-button');
        const activeTabIndex = Array.from(tabButtons).findIndex(tab => tab.classList.contains('active'));
        
        if (e.key === 'ArrowLeft' && activeTabIndex > 0) {
            tabButtons[activeTabIndex - 1].click();
        } else if (e.key === 'ArrowRight' && activeTabIndex < tabButtons.length - 1) {
            tabButtons[activeTabIndex + 1].click();
        }
    });
});

// Add pulse animation for recommended badge
const style = document.createElement('style');
style.textContent = `
    @keyframes pulse {
        0%, 100% { transform: scale(1); }
        50% { transform: scale(1.1); }
    }
`;
document.head.appendChild(style);
</script>

	
