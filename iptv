    <!-- Enhanced FAQ Section with Microdata -->
    <section class="faq" id="faq" itemscope itemtype="https://schema.org/FAQPage">
        <div class="container">
            <div class="text-center" style="margin-bottom: var(--space-4xl);">
                <h1 itemprop="name">Frequently Asked <span class="gradient-text">Questions</span></h1>
                <p style="color: var(--text-secondary); font-size: 1.125rem; max-width: 700px; margin: var(--space-lg) auto 0;" itemprop="description">
                    üí° Get instant answers to the most common questions about our premium IPTV service.
                </p>
            </div>
            
            <div class="faq-container">
                <!-- FAQ Item 1 -->
                <div class="faq-item" itemscope itemprop="mainEntity" itemtype="https://schema.org/Question">
                    <button class="faq-question" aria-expanded="false" aria-controls="faq-answer-1">
                        <span itemprop="name">What is IPTV and how does it work?</span>
                        <i class="fas fa-chevron-down faq-icon" aria-hidden="true"></i>
                    </button>
