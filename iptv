    <script>
        // Show WhatsApp button on scroll
        window.onscroll = function() {
            let button = document.getElementById('whatsapp_button');
            if (document.body.scrollTop > 100 || document.documentElement.scrollTop > 100) {
                button.style.display = 'flex';
            } else {
                button.style.display = 'none';
            }
        };
    </script>
	

    <!-- Back to Top Button  -->
    <button class="back-to-top" id="backToTop" aria-label="Back to Top">
        <i class="fas fa-chevron-up"></i>
    </button>

    <!-- Enhanced JavaScript for Premium Experience -->
    <script>
        // Advanced Performance Management
        class PerformanceManager {
            constructor() {
                this.observers = new Map();
                this.init();
            }

            init() {
                this.setupIntersectionObserver();
                this.setupPerformanceMonitoring();
                this.preloadCriticalResources();
            }

            setupIntersectionObserver() {
                const observerOptions = {
                    threshold: [0.1, 0.5, 0.9],
                    rootMargin: '50px 0px -50px 0px'
                };

                const observer = new IntersectionObserver((entries) => {
                    entries.forEach(entry => {
                        if (entry.isIntersecting) {
                            this.animateElement(entry.target);
                        }
                    });
                }, observerOptions);

                // Observe all animatable elements
                const elements = document.querySelectorAll('.channel-item, .feature-item');
                elements.forEach((el, index) => {
                    el.style.opacity = '0';
                    el.style.transform = 'translateY(40px)';
                    el.style.transition = `all 0.3s ease-out ${index * 0.05}s`;
                    observer.observe(el);
                });
            }

            animateElement(element) {
                element.style.opacity = '1';
                element.style.transform = 'translateY(0)';
            }

            setupPerformanceMonitoring() {
                if ('performance' in window) {
                    window.addEventListener('load', () => {
                        setTimeout(() => {
                            const navigation = performance.getEntriesByType('navigation')[0];
                            const paintEntries = performance.getEntriesByType('paint');
                            
                            const metrics = {
                                loadTime: Math.round(navigation.loadEventEnd - navigation.loadEventStart),
                                domContentLoaded: Math.round(navigation.domContentLoadedEventEnd - navigation.domContentLoadedEventStart),
                                firstPaint: paintEntries.find(entry => entry.name === 'first-paint')?.startTime || 0,
                                firstContentfulPaint: paintEntries.find(entry => entry.name === 'first-contentful-paint')?.startTime || 0
                            };
                            
                            console.log('üöÄ Performance Metrics:', metrics);
                            
                            // Track performance with analytics
                            if (typeof gtag !== 'undefined') {
                                gtag('event', 'page_performance', {
                                    'load_time': metrics.loadTime,
                                    'fcp': Math.round(metrics.firstContentfulPaint),
                                    'event_category': 'performance'
                                });
                            }
                        }, 1000);
                    });
                }
            }

            preloadCriticalResources() {
                // Preload critical images
                const criticalImages = [
                    'https://ipplaytv.com/ar/img/all-channel.webp'
                ];
                
                criticalImages.forEach(src => {
                    const link = document.createElement('link');
                    link.rel = 'preload';
                    link.as = 'image';
                    link.href = src;
                    document.head.appendChild(link);
                });
            }
        }

        // Advanced UI Manager
        class UIManager {
            constructor() {
                this.elements = {
                    loading: document.getElementById('loading'),
                    header: document.getElementById('header'),
                    backToTop: document.getElementById('backToTop'),
                    mobileToggle: document.getElementById('mobileToggle'),
                    languageBtn: document.getElementById('languageBtn'),
                    languageDropdown: document.getElementById('languageDropdown'),
                    deviceCards: document.querySelectorAll('.device-card'),
                    faqItems: document.querySelectorAll('.faq-item'),
                    pricingCards: document.querySelectorAll('.pricing-card')
                };
                this.init();
            }

            init() {
                this.setupLoadingScreen();
                this.setupSmoothScrolling();
                this.setupHeaderEffects();
                this.setupDeviceSelection();
                this.setupFAQ();
                this.setupBackToTop();
                this.setupLanguageSelector();
                this.setupMobileMenu();
                this.setupKeyboardNavigation();
            }

            setupLanguageSelector() {
                if (this.elements.languageBtn && this.elements.languageDropdown) {
                    this.elements.languageBtn.addEventListener('click', (e) => {
                        e.stopPropagation();
                        this.toggleLanguageDropdown();
                    });

                    // Close dropdown when clicking outside
                    document.addEventListener('click', (e) => {
                        if (!this.elements.languageBtn.contains(e.target) && 
                            !this.elements.languageDropdown.contains(e.target)) {
                            this.closeLanguageDropdown();
                        }
                    });

                    // Handle language selection
                    this.elements.languageDropdown.addEventListener('click', (e) => {
                        if (e.target.classList.contains('language-option') || 
                            e.target.closest('.language-option')) {
                            const option = e.target.closest('.language-option');
                            this.selectLanguage(option);
                        }
                    });
                }
            }

            toggleLanguageDropdown() {
                const isOpen = this.elements.languageDropdown.classList.contains('active');
                this.elements.languageBtn.classList.toggle('active', !isOpen);
                this.elements.languageDropdown.classList.toggle('active', !isOpen);
            }

            closeLanguageDropdown() {
                this.elements.languageBtn.classList.remove('active');
                this.elements.languageDropdown.classList.remove('active');
            }

            selectLanguage(option) {
                const langCode = option.dataset.lang.toUpperCase();
                const currentLang = this.elements.languageBtn.querySelector('.current-lang');
                
                // Update active state
                this.elements.languageDropdown.querySelectorAll('.language-option').forEach(opt => {
                    opt.classList.remove('active');
                });
                option.classList.add('active');
                
                // Update button text
                currentLang.textContent = langCode;
                
                // Close dropdown
                this.closeLanguageDropdown();
                
                // Track language change
                if (typeof gtag !== 'undefined') {
                    gtag('event', 'language_change', {
                        'language': langCode.toLowerCase(),
                        'event_category': 'engagement'
                    });
                }
            }

            setupMobileMenu() {
                if (this.elements.mobileToggle) {
                    this.elements.mobileToggle.addEventListener('click', (e) => {
                        e.stopPropagation();
                        this.toggleMobileMenu();
                    });
                }
            }

            toggleMobileMenu() {
                this.elements.mobileToggle.classList.toggle('active');
                // Add mobile menu functionality here if needed
            }

            setupLoadingScreen() {
                window.addEventListener('load', () => {
                    setTimeout(() => {
                        this.elements.loading.classList.add('hidden');
                    }, 1500);
                });
            }

            setupSmoothScrolling() {
                document.querySelectorAll('a[href^="#"]').forEach(anchor => {
                    anchor.addEventListener('click', (e) => {
                        e.preventDefault();
                        const targetId = anchor.getAttribute('href');
                        if (targetId === '#') return;

                        const targetElement = document.querySelector(targetId);
                        if (targetElement) {
                            const headerHeight = this.elements.header.offsetHeight;
                            const targetPosition = targetElement.offsetTop - headerHeight - 20;
                            
                            window.scrollTo({
                                top: targetPosition,
                                behavior: 'smooth'
                            });

                            // Track navigation
                            if (typeof gtag !== 'undefined') {
                                gtag('event', 'navigation', {
                                    'section': targetId.replace('#', ''),
                                    'event_category': 'engagement'
                                });
                            }
                        }
                    });
                });
            }

            setupHeaderEffects() {
                let ticking = false;
                
                window.addEventListener('scroll', () => {
                    if (!ticking) {
                        requestAnimationFrame(() => {
                            this.handleScroll();
                            ticking = false;
                        });
                        ticking = true;
                    }
                }, { passive: true });
            }

            handleScroll() {
                const scrollY = window.pageYOffset;
                
                // Header effects
                if (scrollY > 100) {
                    this.elements.header.style.background = 'rgba(0, 0, 0, 0.98)';
                    this.elements.header.style.boxShadow = '0 10px 30px rgba(0, 0, 0, 0.3)';
                    this.elements.header.style.backdropFilter = 'blur(20px)';
                } else {
                    this.elements.header.style.background = 'rgba(0, 0, 0, 0.95)';
                    this.elements.header.style.boxShadow = 'none';
                }

                // Back to top button
                if (scrollY > 500) {
                    this.elements.backToTop.classList.add('show');
                } else {
                    this.elements.backToTop.classList.remove('show');
                }
            }

            setupDeviceSelection() {
                this.activeDevice = 0;
                
                this.elements.deviceCards.forEach((card, index) => {
                    card.addEventListener('click', () => {
                        this.selectDevice(index);
                    });
                });
            }

            selectDevice(index) {
                // Remove active class from all cards
                this.elements.deviceCards.forEach(card => card.classList.remove('active'));
                
                // Add active class to selected card
                this.elements.deviceCards[index].classList.add('active');
                
                this.activeDevice = index;
                this.updatePricing(index);
                
                // Track selection
                if (typeof gtag !== 'undefined') {
                    gtag('event', 'device_selection', {
                        'device_count': index + 1,
                        'event_category': 'pricing'
                    });
                }

                // Add visual feedback
                const card = this.elements.deviceCards[index];
                card.style.transform = 'scale(1.1)';
                setTimeout(() => {
                    card.style.transform = '';
                }, 200);
            }

            updatePricing(deviceIndex) {
                const basePrices = [
                    { monthly: 9.99, quarterly: 24.99, semiannual: 39.99, annual: 59.99 },
                    { monthly: 19.99, quarterly: 33.99, semiannual: 55.99, annual: 84.99 },
                    { monthly: 30.99, quarterly: 52.99, semiannual: 84.99, annual: 127.99 },
                    { monthly: 40.99, quarterly: 67.99, semiannual: 111.99, annual: 169.99 },
                    { monthly: 49.99, quarterly: 84.99, semiannual: 140.99, annual: 212.99 }
                ];

                const currentPrices = basePrices[deviceIndex];
                
                // Update pricing display with animation
                const priceElements = document.querySelectorAll('.plan-price');
                priceElements.forEach((element, index) => {
                    const prices = Object.values(currentPrices);
                    if (prices[index]) {
                        element.style.transform = 'scale(0.8)';
                        element.style.opacity = '0.5';
                        
                        setTimeout(() => {
                            element.textContent = `${prices[index]}`;
                            element.style.transform = 'scale(1)';
                            element.style.opacity = '1';
                        }, 150);
                    }
                });
            }

            setupFAQ() {
                this.elements.faqItems.forEach(item => {
                    const question = item.querySelector('.faq-question');
                    const answer = item.querySelector('.faq-answer');
                    
                    question.addEventListener('click', () => {
                        const isActive = item.classList.contains('active');
                        
                        // Close all other FAQ items
                        this.elements.faqItems.forEach(otherItem => {
                            if (otherItem !== item) {
                                otherItem.classList.remove('active');
                            }
                        });
                        
                        // Toggle current item
                        item.classList.toggle('active', !isActive);
                        
                        // Track FAQ interaction
                        if (!isActive && typeof gtag !== 'undefined') {
                            gtag('event', 'faq_interaction', {
                                'question': question.textContent.trim(),
                                'event_category': 'engagement'
                            });
                        }
                    });
                });
            }

            setupBackToTop() {
                this.elements.backToTop.addEventListener('click', () => {
                    window.scrollTo({ 
                        top: 0, 
                        behavior: 'smooth' 
                    });
                    
                    if (typeof gtag !== 'undefined') {
                        gtag('event', 'back_to_top', {
                            'event_category': 'navigation'
                        });
                    }
                });
            }

            setupKeyboardNavigation() {
                // Enhanced accessibility
                document.addEventListener('keydown', (e) => {
                    if (e.key === 'Escape') {
                        // Close any open modals or dropdowns
                        this.elements.faqItems.forEach(item => {
                            item.classList.remove('active');
                        });
                    }
                });
            }
        }

        // Advanced Analytics Manager
        class AnalyticsManager {
            constructor() {
                this.events = [];
                this.init();
            }

            init() {
                this.setupCTATracking();
                this.setupScrollTracking();
                this.setupEngagementTracking();
                this.setupErrorTracking();
            }

            setupCTATracking() {
                document.querySelectorAll('.btn').forEach(button => {
                    button.addEventListener('click', (e) => {
                        const buttonText = e.target.textContent.trim();
                        const section = e.target.closest('section')?.id || 'unknown';
                        
                        this.trackEvent('cta_click', {
                            'button_text': buttonText,
                            'section': section,
                            'event_category': 'conversion'
                        });
                    });
                });
            }

            setupScrollTracking() {
                let maxScroll = 0;
                const milestones = [25, 50, 75, 90, 100];
                const tracked = new Set();

                window.addEventListener('scroll', () => {
                    const scrollPercent = Math.round(
                        (window.scrollY / (document.body.scrollHeight - window.innerHeight)) * 100
                    );
                    
                    if (scrollPercent > maxScroll) {
                        maxScroll = scrollPercent;
                        
                        milestones.forEach(milestone => {
                            if (scrollPercent >= milestone && !tracked.has(milestone)) {
                                tracked.add(milestone);
                                this.trackEvent('scroll_depth', {
                                    'percentage': milestone,
                                    'event_category': 'engagement'
                                });
                            }
                        });
                    }
                }, { passive: true });
            }

            setupEngagementTracking() {
                // Track time spent on page
                let timeSpent = 0;
                const interval = setInterval(() => {
                    timeSpent += 10;
                    if (timeSpent % 30 === 0) { // Every 30 seconds
                        this.trackEvent('time_on_page', {
                            'seconds': timeSpent,
                            'event_category': 'engagement'
                        });
                    }
                }, 10000);

                // Track when user leaves
                window.addEventListener('beforeunload', () => {
                    clearInterval(interval);
                    this.trackEvent('session_end', {
                        'total_time': timeSpent,
                        'event_category': 'engagement'
                    });
                });
            }

            setupErrorTracking() {
                window.addEventListener('error', (e) => {
                    this.trackEvent('javascript_error', {
                        'error_message': e.error?.message || 'Unknown error',
                        'error_filename': e.filename || 'unknown',
                        'error_line': e.lineno || 'unknown',
                        'event_category': 'error'
                    });
                });

                window.addEventListener('unhandledrejection', (e) => {
                    this.trackEvent('promise_rejection', {
                        'error_message': e.reason?.message || 'Unknown rejection',
                        'event_category': 'error'
                    });
                });
            }

            trackEvent(eventName, parameters) {
                this.events.push({ eventName, parameters, timestamp: Date.now() });
                
                if (typeof gtag !== 'undefined') {
                    gtag('event', eventName, parameters);
                }
                
                // Console log for debugging
                console.log(`üìä Event: ${eventName}`, parameters);
            }
        }

        // Initialize all managers when DOM is ready
        document.addEventListener('DOMContentLoaded', () => {
            new PerformanceManager();
            new UIManager();
            new AnalyticsManager();
            
            console.log('üöÄ  BEST IPTV RESELLERS - Premium Experience Loaded!');
        });

        // Service Worker Registration for PWA
        if ('serviceWorker' in navigator) {
            window.addEventListener('load', () => {
                navigator.serviceWorker.register('/sw.js')
                    .then(registration => {
                        console.log('‚úÖ Service Worker registered successfully');
                    })
                    .catch(registrationError => {
                        console.log('‚ùå Service Worker registration failed:', registrationError);
                    });
            });
        }

        // Enhanced Security and Performance
        document.addEventListener('contextmenu', e => e.preventDefault()); // Disable right-click
        document.addEventListener('selectstart', e => e.preventDefault()); // Disable text selection
        document.addEventListener('dragstart', e => e.preventDefault()); // Disable drag
    </script>

