/* Animations */
@keyframes regionCardFloat {
    0%, 100% { transform: translateY(0px); }
    50% { transform: translateY(-5px); }
}

.region-cards .region-card:nth-child(odd) {
    animation: regionCardFloat 6s ease-in-out infinite;
}

.region-cards .region-card:nth-child(even) {
    animation: regionCardFloat 6s ease-in-out infinite reverse;
}

/* Intersection Observer Animation Classes */
.region-card {
    opacity: 0;
    transform: translateY(50px);
    transition: all 0.6s cubic-bezier(0.4, 0, 0.2, 1);
}

.region-card.animate-in {
    opacity: 1;
    transform: translateY(0);
}

.region-card:nth-child(1) { transition-delay: 0.1s; }
.region-card:nth-child(2) { transition-delay: 0.2s; }
.region-card:nth-child(3) { transition-delay: 0.3s; }
.region-card:nth-child(4) { transition-delay: 0.4s; }
.region-card:nth-child(5) { transition-delay: 0.5s; }
.region-card:nth-child(6) { transition-delay: 0.6s; }
.region-card:nth-child(7) { transition-delay: 0.7s; }
.region-card:nth-child(8) { transition-delay: 0.8s; }
</style>

<!-- Enhanced JavaScript for Regional Cards -->
<script>
document.addEventListener('DOMContentLoaded', function() {
    // Initialize Regional Navigation Features
    initRegionalNavigation();
    
    function initRegionalNavigation() {
        setupIntersectionObserver();
        setupRegionalTracking();
        setupHoverEffects();
        setupCurrencyDisplay();
    }
    
    // Intersection Observer for Animation
    function setupIntersectionObserver() {
        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.classList.add('animate-in');
                }
            });
        }, {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        });
        
        document.querySelectorAll('.region-card').forEach(card => {
            observer.observe(card);
        });
    }
    
    // Enhanced Click Tracking
    function setupRegionalTracking() {
        document.querySelectorAll('.region-card').forEach(card => {
            card.addEventListener('click', function(e) {
                const region = this.getAttribute('data-region');
                const price = this.querySelector('.price').textContent;
                
                // Track with analytics
                if (typeof gtag !== 'undefined') {
                    gtag('event', 'region_selection', {
                        'region': region,
                        'price': price,
                        'event_category': 'navigation',
                        'value': parseFloat(price.replace(/[^0-9.]/g, ''))
                    });
                }
                
                // Visual feedback
                this.style.transform = 'scale(0.98)';
                setTimeout(() => {
                    this.style.transform = '';
                }, 150);
                
                console.log(`üåç User selected ${region} region for ${price}`);
            });
        });
    }
    
    // Enhanced Hover Effects
    function setupHoverEffects() {
        document.querySelectorAll('.region-card').forEach(card => {
            card.addEventListener('mouseenter', function() {
                // Add subtle glow effect
                this.style.boxShadow = `
                    0 20px 40px rgba(0, 0, 0, 0.3),
                    0 0 30px rgba(59, 130, 246, 0.2),
                    inset 0 1px 0 rgba(255, 255, 255, 0.1)
                `;
                
                // Animate channel tags
                this.querySelectorAll('.channel-tag').forEach((tag, index) => {
                    setTimeout(() => {
                        tag.style.transform = 'scale(1.05)';
                        tag.style.backgroundColor = 'rgba(59, 130, 246, 0.2)';
                    }, index * 50);
                });
            });
            
            card.addEventListener('mouseleave', function() {
                // Reset effects
                this.style.boxShadow = '';
                
                this.querySelectorAll('.channel-tag').forEach(tag => {
                    tag.style.transform = '';
                    tag.style.backgroundColor = '';
                });
            });
        });
    }
    

    
    // Performance monitoring
    window.addEventListener('load', () => {
        setTimeout(() => {
            const regionalSection = document.querySelector('.regional-quick-nav');
            if (regionalSection) {
                console.log('üöÄ Regional navigation loaded successfully');
                
                // Track performance
                if (typeof gtag !== 'undefined') {
                    gtag('event', 'regional_nav_loaded', {
                        'event_category': 'performance',
                        'timing': Date.now()
                    });
                }
            }
        }, 1000);
    });
});
</script>



