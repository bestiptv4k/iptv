	<!-- Simple Payment Links JavaScript (No Device Indicator) -->
<script>
document.addEventListener('DOMContentLoaded', function() {
    
    // üöÄ Simple Payment Links System
    class SimplePaymentManager {
        constructor() {
            this.activeDeviceCount = 1;
            this.deviceCards = document.querySelectorAll('.device-card');
            this.pricingButtons = document.querySelectorAll('.pricing-btn');
            
            // üîß CONFIGURE YOUR PAYMENT LINKS HERE
            this.paymentLinks = {
                // 1 MONTH PLAN (data-plan="1")
                "1_1": "https://ipplaytv.com/checkout/?id=115&currency=USD&lang=en",      // 1 month, 1 device
                "1_2": "https://ipplaytv.com/checkout/?id=119&currency=USD&lang=en",     // 1 month, 2 devices
                "1_3": "https://ipplaytv.com/checkout/?id=123&currency=USD&lang=en",      // 1 month, 3 devices
                "1_4": "https://ipplaytv.com/checkout/?id=127&currency=USD&lang=en",    // 1 month, 4 devices
                "1_5": "https://ipplaytv.com/checkout/?id=131&currency=USD&lang=en",     // 1 month, 5 devices
                
                // 3 MONTHS PLAN (data-plan="2")
                "2_1": "https://ipplaytv.com/checkout/?id=116&currency=USD&lang=en",    // 3 months, 1 device
                "2_2": "https://ipplaytv.com/checkout/?id=120&currency=USD&lang=en",     // 3 months, 2 devices
                "2_3": "https://ipplaytv.com/checkout/?id=124&currency=USD&lang=en",      // 3 months, 3 devices
                "2_4": "https://ipplaytv.com/checkout/?id=128&currency=USD&lang=en",    // 3 months, 4 devices
                "2_5": "https://ipplaytv.com/checkout/?id=132&currency=USD&lang=en",     // 3 months, 5 devices
                
                // 6 MONTHS PLAN (data-plan="3")
                "3_1": "https://ipplaytv.com/checkout/?id=117&currency=USD&lang=en",    // 6 months, 1 device
                "3_2": "https://ipplaytv.com/checkout/?id=121&currency=USD&lang=en",     // 6 months, 2 devices
                "3_3": "https://ipplaytv.com/checkout/?id=125&currency=USD&lang=en",      // 6 months, 3 devices
                "3_4": "https://ipplaytv.com/checkout/?id=129&currency=USD&lang=en",    // 6 months, 4 devices
                "3_5": "https://ipplaytv.com/checkout/?id=133&currency=USD&lang=en",     // 6 months, 5 devices
                
                // 12 MONTHS PLAN (data-plan="4")
                "4_1": "https://ipplaytv.com/checkout/?id=118&currency=USD&lang=en",    // 12 months, 1 device
                "4_2": "https://ipplaytv.com/checkout/?id=122&currency=USD&lang=en",     // 12 months, 2 devices
                "4_3": "https://ipplaytv.com/checkout/?id=126&currency=USD&lang=en",      // 12 months, 3 devices
                "4_4": "https://ipplaytv.com/checkout/?id=130&currency=USD&lang=en",    // 12 months, 4 devices
                "4_5": "https://ipplaytv.com/checkout/?id=134&currency=USD&lang=en"      // 12 months, 5 devices
            };

            // üí∞ PRICING MATRIX
            this.pricingMatrix = {
                1: [9.99, 24.99, 39.99, 59.99],   // 1 device
                2: [19.99, 33.99, 55.99, 84.99],  // 2 devices
                3: [30.99, 52.99, 84.99, 127.99], // 3 devices
                4: [40.99, 67.99, 111.99, 169.99], // 4 devices
                5: [49.99, 84.99, 140.99, 212.99]  // 5 devices
            };
            
            this.init();
        }

        init() {
            this.setupDeviceSelection();
            this.setupPaymentButtons();
            this.updatePaymentLinks(1); // Start with 1 device
            console.log('üí≥ Simple Payment Manager Ready!');
        }

        setupDeviceSelection() {
            this.deviceCards.forEach((card) => {
                card.addEventListener('click', () => {
                    const deviceCount = parseInt(card.getAttribute('data-devices'));
                    this.selectDevice(deviceCount);
                });
            });
        }

        selectDevice(deviceCount) {
            // Update active device selection
            this.deviceCards.forEach(card => card.classList.remove('active'));
            const selectedCard = document.querySelector(`[data-devices="${deviceCount}"]`);
            if (selectedCard) {
                selectedCard.classList.add('active');
                
                // Visual feedback
                selectedCard.style.transform = 'scale(1.05)';
                setTimeout(() => {
                    selectedCard.style.transform = '';
                }, 200);
            }
            
            this.activeDeviceCount = deviceCount;
            this.updatePricing(deviceCount);
            this.updatePaymentLinks(deviceCount);
            
            console.log(`üì± Selected ${deviceCount} device(s)`);
        }

        updatePaymentLinks(deviceCount) {
            this.pricingButtons.forEach(button => {
                const plan = button.getAttribute('data-plan');
                const linkKey = `${plan}_${deviceCount}`;
                const paymentUrl = this.paymentLinks[linkKey];
                
                if (paymentUrl) {
                    button.href = paymentUrl;
                    button.style.opacity = '1';
                    button.style.pointerEvents = 'auto';
                    button.removeAttribute('disabled');
                    
                    // Visual feedback
                    button.style.boxShadow = '0 0 15px rgba(59, 130, 246, 0.4)';
                    setTimeout(() => {
                        button.style.boxShadow = '';
                    }, 800);
                    
                    console.log(`‚úÖ Plan ${plan} ‚Üí ${paymentUrl}`);
                } else {
                    button.href = '#';
                    button.style.opacity = '0.5';
                    button.style.pointerEvents = 'none';
                    console.warn(`‚ö†Ô∏è No link for Plan ${plan}, ${deviceCount} devices`);
                }
            });
        }

        updatePricing(deviceCount) {
            const prices = this.pricingMatrix[deviceCount];
            if (!prices) return;

            const priceElements = document.querySelectorAll('.plan-price');
            priceElements.forEach((element, index) => {
                if (prices[index]) {
                    element.style.transform = 'scale(0.9)';
                    element.style.opacity = '0.7';
                    
                    setTimeout(() => {
                        element.textContent = `$${prices[index]}`;
                        element.style.transform = 'scale(1)';
                        element.style.opacity = '1';
                    }, 100);
                }
            });
        }

        setupPaymentButtons() {
            this.pricingButtons.forEach(button => {
                button.addEventListener('click', (e) => {
                    const url = button.getAttribute('href');
                    const plan = button.getAttribute('data-plan');
                    
                    if (url === '#' || button.hasAttribute('disabled')) {
                        e.preventDefault();
                        alert('Payment option not available. Please select a different plan or device count.');
                        return;
                    }
                    
                    console.log(`üí∞ Payment: Plan ${plan} for ${this.activeDeviceCount} device(s) ‚Üí ${url}`);
                });
            });
        }
    }

    // Initialize the payment manager
    const paymentManager = new SimplePaymentManager();

    // Keyboard shortcuts (1-5 keys)
    document.addEventListener('keydown', function(e) {
        if (e.key >= '1' && e.key <= '5') {
            const deviceCount = parseInt(e.key);
            paymentManager.selectDevice(deviceCount);
        }
    });

    // Make it globally accessible
    window.paymentManager = paymentManager;

    console.log('üöÄ IPTV Payment System Loaded!');
    console.log('üí° Press 1-5 keys to select devices quickly');
});
</script>
	

